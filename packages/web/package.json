{
  "name": "pob-web",
  "private": true,
  "version": "0.8.0",
  "type": "module",
  "dependencies": {
    "@auth0/auth0-react": "^2.2.4",
    "@sentry/react": "^7.114.0",
    "jose": "^5.3.0",
    "missionlog": "^1.8.8",
    "pob-driver": "*",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-use": "^17.5.0",
    "recoil": "^0.7.7"
  },
  "scripts": {
    "dev": "concurrently vite \"wrangler pages dev . --compatibility-date=2024-05-11\"",
